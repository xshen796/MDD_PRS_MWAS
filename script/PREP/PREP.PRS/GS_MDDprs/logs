/exports/igmm/eddie/GenScotDepression/shen/Tools/PRSice/PRSice_linux \
    --a1 A1 \
    --a2 A2 \
    --all-score  \
    --allow-inter  \
    --bar-levels 5e-08,1 \
    --base /exports/igmm/eddie/GenScotDepression/shen/bakup.dat/summstats/23andme_PGCNoGS_UKB_Aug8_FinalGCldsc_3cohort1.meta.forPRSice.gz \
    --beta  \
    --binary-target T \
    --bp BP \
    --chr CHR \
    --extract PRSice.valid \
    --fastscore  \
    --no-clump  \
    --nonfounders  \
    --num-auto 22 \
    --out /exports/eddie/scratch/xshen33/GS_PRS_loo/PRS_5e_08/MDDprs_5e_08 \
    --pheno /exports/igmm/eddie/GenScotDepression/shen/ActiveProject/Genetic/MDD_PRS_MWAS/data/GS_loo_PRS/dummy.MDD.GS \
    --pheno-col dummy_MDD \
    --print-snp  \
    --pvalue P \
    --seed 236440401 \
    --snp SNP \
    --stat BETA \
    --target /exports/eddie/scratch/xshen33/GS_PRS_loo/data/GS20K_HRC_0.8_GCTA_nodupvar \
    --thread 5

